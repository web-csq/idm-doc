#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_markdown-it@12.3.2@markdown-it/bin/markdown-it.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_markdown-it@12.3.2@markdown-it/bin/markdown-it.js" "$@"
  ret=$?
fi
exit $ret
